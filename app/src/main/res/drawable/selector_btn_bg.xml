<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="@dimen/dimens8" />
            <gradient android:startColor="#1eaef1"
                android:endColor="#70c6ed"
                android:angle="90" />
            <padding android:top="@dimen/dimens2" android:bottom="@dimen/dimens2"
                android:left="@dimen/dimens2" android:right="@dimen/dimens2" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/dimens8" />
            <gradient android:startColor="#01a6f2"
                android:endColor="#3396c2"
                android:angle="90" />
            <padding android:top="@dimen/dimens2" android:bottom="@dimen/dimens2"
                android:left="@dimen/dimens2" android:right="@dimen/dimens2" />
        </shape>
    </item>
</selector>